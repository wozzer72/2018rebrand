{
  "parser": "babel-eslint",
  "extends": [
    "prettier"
  ],
  "settings": {
    "import/resolver": {
    "webpack": {
      "config": "./webpack.config.js"
      }
    }
  },
  "rules": {
    "comma-dangle": 0,
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "wyze/sort-destructuring-keys": 0,
    "arrow-parens": 0,
    "max-len": [
      "error",
      120
    ],
    "arrow-body-style": 0,
    "no-console": 0,
    "padded-blocks": 0,
    "newline-per-chained-call": 0,
    "import/prefer-default-export": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 1,
    "import/no-named-as-default": 0,
  },
  "globals": {
    "document": true,
    "window": true
  },
  "env": {
    "node": true,
    "jest": true
  },
  "plugins": [
    "jest",
    "wyze",
    "import"
  ]
}
